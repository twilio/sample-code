{
  "title": "Update your assistant defaults.",
  "type": "server",
  "_definition": {
    "params": {
      "required": {
        "defaults": {
          "defaults": {
            "assistant_initiation": "task://hello-world",
            "fallback": "task://hello-world"
          }
        }
      },
      "path": {
        "assistant_sid": "UAXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"
      }
    },
    "location": "https://autopilot.twilio.com/v1/Assistants/{assistant_sid}/Defaults",
    "method": "post",
    "domain": "autopilot",
    "version": "v1",
    "resource": "defaults",
    "action": "update"
  }
}